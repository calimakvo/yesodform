                        <button class="btn btn-primary" type="submit">Upload

                <div class="clearfix">
                    <h1>AForm Horizon
                    <form action="" method="post" enctype="#{formAEnctypeHorizon}">
                        ^{formAWidgetHorizon}
                        <div class="form-group">
                            <div class="col-sm-offset-2 col-sm-10">
                                <button class="btn btn-primary" type="submit">Upload

                <h1>MForm
                <form action="" method="post" enctype="#{enctype}">
                    ^{widget}
                    <div class="form-group">
                        <button class="btn btn-primary" type="submit">Upload
    <div .bs-docs-section>
        <div .row>
            <div .col-lg-12>
                <h1>AForm BasicForm
                <form action="" method="post" enctype="#{formAEnctypeBasic}">
                    ^{formAWidgetBasic}
                     <div class="form-group">
                         <button class="btn btn-primary" type="submit">Upload

                <h1>AForm InlineForm
                <form action="" method="post" enctype="#{formAEnctypeInline}" class="form-inline">
                    ^{formAWidgetInline}
